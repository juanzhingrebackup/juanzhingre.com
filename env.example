# Textbelt API Key for SMS Service
# Get your API key from: https://textbelt.com/
TEXTBELT_KEY=your_textbelt_api_key_here

# Database Connection String (Neon PostgreSQL)
# Get your connection string from: https://console.neon.tech/
DATABASE_URL=postgresql://user:password@host/database

# Google Maps API Key
# Get your API key from: https://console.cloud.google.com/google/maps-apis
GOOGLE_MAPS_KEY=your_google_maps_api_key_here

# Business Contact Information (Public - visible to clients)
NEXT_PUBLIC_BUSINESS_PHONE=+1234567890
NEXT_PUBLIC_BUSINESS_ADDRESS=1234 Apple Ln, Provo, UT

# Service Area, Google Maps will use this to calculate distances
NEXT_PUBLIC_SERVICE_AREA=Provo, UT
